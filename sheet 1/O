#include <iostream>
using namespace std;
int main() {
    int num1, num2;
    char s;
    cin>>num1>>s>>num2;
    if (s == '+')
    cout<<num1+num2;
    if (s == '-')
    cout<<num1-num2;
    if (s == '*')
    cout<<num1*num2;
    if (s == '/')
    cout<<num1/num2;
    return 0;
}
#include <iostream>
using namespace std;
int main() {
    int num1, num2;
    char s;
    cin>>num1>>s>>num2;
    if (s == 43)
      cout<<num1+num2;
    if (s == 45)
      cout<<num1-num2;
    if (s == 42)
      cout<<num1*num2;
    if (s == 47)
      cout<<num1/num2;
    return 0;
}
