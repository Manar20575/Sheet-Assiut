// Online C++ compiler to run C++ program online
#include <iostream>
#include <cmath>
using namespace std;
int main() {
int inte, l1, r1, l2, r2, f1, e1;
cin >> l1 >> r1 >> l2 >> r2; //291882089 585325539 358502890 365329221
f1 =  max(l1,l2);
e1 = min(r1,r2);
//cout << " f1"<< f1 << "e1"<<e1;
if (l1<=r1 && l2<=r2 && f1<=e1){
    if(l2>=l1 && r1<=r2){
         cout <<f1<<" "<< e1;
    }
     else if (l1>=l2 && r1>=r2){
                cout <<f1<<" "<< e1;
     }
     else if (l1>=l2 && r1<=r2){
                cout <<f1<<" "<< e1;
     }
     else if (l1<=l2 && r1>=r2){
                cout <<f1<<" "<< e1;
     }
}
else
cout <<-1;
    return 0;
}
